{
    "version": "3.8",
    "services:cloudreve:container_name": "cloudreve",
    "services:cloudreve:image": "cloudreve\/cloudreve:latest",
    "services:cloudreve:restart": "unless-stopped",
    "services:cloudreve:networks:0": "share_net",
    "services:cloudreve:ports:0": "5212:5212",
    "services:cloudreve:volumes:0": "temp_data:\/data",
    "services:cloudreve:volumes:1": "\/usr\/local\/cloudreve\/cloudreve\/uploads:\/cloudreve\/uploads",
    "services:cloudreve:volumes:2": "\/usr\/local\/cloudreve\/cloudreve\/conf.ini:\/cloudreve\/conf.ini",
    "services:cloudreve:volumes:3": "\/usr\/local\/cloudreve\/cloudreve\/cloudreve.db:\/cloudreve\/cloudreve.db",
    "services:cloudreve:volumes:4": "\/usr\/local\/cloudreve\/cloudreve\/avatar:\/cloudreve\/avatar",
    "services:cloudreve:depends_on:0": "aria2",
    "services:aria2:container_name": "aria2",
    "services:aria2:image": "p3terx\/aria2-pro",
    "services:aria2:restart": "unless-stopped",
    "services:aria2:networks:0": "share_net",
    "services:aria2:environment:0": "RPC_SECRET=your_aria_rpc_token",
    "services:aria2:environment:1": "RPC_PORT=6800",
    "services:aria2:volumes:0": "\/usr\/local\/cloudreve\/aria2\/config:\/config",
    "services:aria2:volumes:1": "temp_data:\/data",
    "services:watchtower:image": "containrrr\/watchtower",
    "services:watchtower:container_name": "watchtower",
    "services:watchtower:restart": "always",
    "services:watchtower:volumes:0": "\/var\/run\/docker.sock:\/var\/run\/docker.sock",
    "services:watchtower:command": "cloudreve aria2 watchtower --cleanup --schedule \"0 30 6 * * *\"",
    "services:watchtower:networks:0": "share_net",
    "volumes:temp_data:driver": "local",
    "volumes:temp_data:driver_opts:type": "none",
    "volumes:temp_data:driver_opts:device": "\/usr\/local\/cloudreve\/data",
    "volumes:temp_data:driver_opts:o": "bind",
    "networks:share_net:driver": "bridge"
}